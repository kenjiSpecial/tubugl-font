{
  "name": "tubugl-font",
  "version": "1.0.0",
  "description": "rendering font with tubugl",
  "main": "./src/main.js",
  "license": "MIT",
  "scripts": {
    "example00": "parcel examples/example00/index.html --no-hmr",
    "example01": "parcel examples/example01/index.html --no-hmr",
    "example02": "parcel examples/example02/index.html --no-hmr",
    "dist00": "parcel build examples/example00/index.html -d docs/example00 --public-url ./",
    "dist01": "parcel build examples/example01/index.html -d docs/example01 --public-url ./",
    "dist02": "parcel build examples/example02/index.html -d docs/example02 --public-url ./",
    "screenshot00": "node ./node/screenshot.js --dir=example00",
    "docs": "python -m SimpleHTTPServer 8000"
  },
  "dependencies": {
    "parcel-plugin-glsl": "kenjiSpecial/parcel-plugin-glsl",
    "tubugl-3d-shape": "^1.3.2",
    "tubugl-camera": "^1.3.2",
    "tubugl-constants": "^1.0.3",
    "tubugl-core": "kenjiSpecial/tubugl-core",
    "tubugl-math": "kenjiSpecial/tubugl-math"
  },
  "devDependencies": {
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "gsap": "^1.20.3",
    "minimist": "^1.2.0",
    "puppeteer": "^0.13.0",
    "stats.js": "mrdoob/stats.js"
  }
}
